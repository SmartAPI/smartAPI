                    }
                },
                "required": [
                    "_id",
                    "taxid"
                ],
                "type": "object"
            },
            "QueryPOSTResult": {
                "items": {
                    "allOf": [
                        {
                            "$ref": "#/components/schemas/MinimalGene"
                        },
                        {
                            "properties": {
                                "_score": {
                                    "format": "float",
