        "x-bte-kgs-operations": {
            "enablesMF": [
                {
                    "inputSeparator": ",",
                    "inputs": [
                        {
                            "id": "NCBIGene",
                            "semantic": "Gene"
                        }
                    ],
                    "outputs": [
                        {
                            "id": "GO",
                            "semantic": "MolecularActivity"
                        }
                    ],
                    "source": "entrez",
                    "predicate": "functional_association",
                    "parameters": {
                        "fields": "go.MF"
                    },
                    "requestBody": {
                        "body": {
                            "q": "{inputs[0]}",
                            "scopes": "entrezgene"
                        },
                        "header": "application/x-www-form-urlencoded"
                    },
                    "supportBatch": true,
                    "response_mapping": {
                        "$ref": "#/components/x-bte-response-mapping/functional_association_mf"
                    }
                }
            ],
            "hasGeneProduct": [
